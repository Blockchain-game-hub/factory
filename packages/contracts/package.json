{
  "name": "@oncyber-scenes/contracts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "node": "hardhat node",
    "test": "npm run test:coverage",
    "test:unit": "hardhat test --logs",
    "test:coverage": "hardhat coverage",
    "lint": "npm run lint:js && npm run lint:sol",
    "lint:js": "eslint \"**/*.{js,ts}\"",
    "lint:sol": "solhint contracts/**/*.sol",
    "lint:fix": "npm run lint:js:fix && npm run lint:sol:fix",
    "lint:js:fix": "eslint --fix \"**/*.{js,ts}\"",
    "lint:sol:fix": "solhint --fix contracts/**/*.sol",
    "prettier": "npm run prettier:js && npm run prettier:sol",
    "prettier:write": "npm run prettier:js:write && npm run prettier:sol:write",
    "prettier:js": "prettier --check .",
    "prettier:js:write": "prettier --write .",
    "prettier:sol": "prettier --check 'contracts/**/*.sol'",
    "prettier:sol:write": "prettier --write 'contracts/**/*.sol'",
    "fix:deps": "sed -i.bak 's/skipSupportsInterface: false,/skipSupportsInterface: true,/' ./node_modules/hardhat-deploy/dist/src/helpers.js",
    "deploy:localhost": "hardhat --network localhost deploy",
    "deploy:mumbai": "hardhat --network mumbai deploy",
    "deploy:mainnet": "hardhat --network mainnet deploy",
    "deploy:rinkeby": "hardhat --network rinkeby deploy",
    "deploy:ethereum": "hardhat --network ethereum deploy",
    "verify:rinkeby": "hardhat etherscan-verify --network rinkeby",
    "verify:ethereum": "hardhat etherscan-verify --network ethereum",
    "verify:CyberDestinationUtilityFactoryFacet:rinkeby": "hardhat verify --network rinkeby 0xAEf4E2F292eBEBA4996a4B011977C5444b3281FD",
    "verify:CyberDestinationUtilityFactoryProxy:rinkeby": "hardhat verify --network rinkeby 0xDeFB45F660166B3FE43Ec110AcaFEBcFC5185b33",
    "verify:CyberDestinationUtilityFactoryFacet:ethereum": "hardhat verify --network ethereum 0xf90fd2bF6E375f9B9528C67329e3cA103Bca2C72",
    "verify:CyberDestinationUtilityFactoryProxy:ethereum": "hardhat verify --network ethereum 0x226Bf5293692610692E2C996C9875C914d2A7f73"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biconomy/mexa": "^2.0.18",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "4.3.1",
    "@solidstate/contracts": "0.0.23",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "chai": "^4.3.4",
    "chai-ethers": "0.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.6",
    "hardhat": "^2.6.2",
    "hardhat-deploy": "^0.9.0",
    "hardhat-deploy-ethers": "^0.3.0-beta.10",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-preprocessor": "^0.1.4",
    "hardhat-tracer": "^1.0.0-alpha.6",
    "hardhat-typechain": "^0.3.5",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-node": "^10.2.1",
    "typechain": "^5.1.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {}
}
